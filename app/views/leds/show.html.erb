                <li>
                    <t>________________________________</t>
                </li>
                <li>

                 <%if user_signed_in?%> 
                  <%= link_to "Sign Out", destroy_user_session_path, method: :delete%></t>
                       
                     <a href="/users/edit">User : <%=current_user.email%></a>
                     
                  <%else%>
                     <a href="/users/sign_in"> Login </a> 
                 
                   
                  <%end%>
                </li>
                
                <li>
                    <t>________________________________</t>
                </li>

                <li>
                    <a href="/leds/<%=@one_ledpanel.id%>" data-method="PUT">UPDATE LED Panel</a>
                </li>
                 <li>
                    <t>________________________________</t>
                </li>
                <li>
